  pod 'FBSDKLoginKit', '~> 4.38.1'
  pod 'FacebookSDK'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'

pod 'Firebase/Auth'

pod 'GoogleSignIn'

end
